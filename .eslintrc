{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": false,
    "react/forbid-prop-types": false,
    "no-console": ["error", { "allow": ["warn", "error"] }] ,
    "jsx-a11y/anchor-is-valid": [ "error", { "components": [ "Link" ] }]
  }
  
}